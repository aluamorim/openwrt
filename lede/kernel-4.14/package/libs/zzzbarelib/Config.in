menu "zzzbarelib"
	select PACKAGE_zlib
	select PACKAGE_libopenssl
	select PACKAGE_baresip
	tristate "Baresip userspace lib"
	default y

config ZZZBARELIB
	tristate "Baresip userspace lib"
	default y
	help
		The barelib package installs the necessary
        .so libraries to run baresip-based applications
        on /usr/lib directory of the image. (ALU)
endmenu


         